{
  "name": "personnummer",
  "description": "Validate Swedish personal identity numbers",
  "version": "3.1.3",
  "license": "MIT",
  "homepage": "https://github.com/personnummer/js",
  "author": {
    "name": "Fredrik Forsmo",
    "email": "fredrik.forsmo@gmail.com",
    "url": "https://frozzare.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/personnummer/js.git"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build": "rimraf dist && npm run build:types && FORMAT=esm node esbuild.js &&  FORMAT=cjs node esbuild.js",
    "format": "prettier --write 'src/**/*.ts' test.ts",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types",
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "core-js": "^3.9.1",
    "esbuild": "^0.10.2",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  },
  "keywords": [
    "personnummer",
    "personal",
    "identity",
    "social",
    "security",
    "numbers"
  ],
  "dependencies": {
    "esbuild-jest": "^0.5.0"
  }
}
